algoritmo "semnome"
var
     num, con, aux: inteiro
inicio
      escreva("A tabuada de qual número você quer ver? ")
      leia(num)
          enquanto con < 10 faca
          con <- con + 1
          aux <- num * con
           escreval(num, " x ", con:2, " = ", aux )
          fimenquanto
fimalgoritmo
