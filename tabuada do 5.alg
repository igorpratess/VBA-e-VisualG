algoritmo "semnome"
var
     cont, aux: inteiro
inicio
      para cont de 1 ate 10 faca
       enquanto cont < 10 faca
       cont <- cont + 1
        repita
         escreval("5", " x ", cont:2, " = ", aux)
	      aux <- 5 * cont
	      ate cont > 10
        fimrepita
       fimenquanto
      fimpara
fimalgoritmo
